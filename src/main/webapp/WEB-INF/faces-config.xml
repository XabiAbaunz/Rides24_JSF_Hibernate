<?xml version="1.0" encoding="UTF-8"?>

<faces-config>
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>mainGUIBean</managed-bean-name>
		<managed-bean-class>eredua.bean.MainGUIBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>eredua.bean.RegisterBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>eredua.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createRideBean</managed-bean-name>
		<managed-bean-class>eredua.bean.CreateRideBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>queryRidesBean</managed-bean-name>
		<managed-bean-class>eredua.bean.QueryRidesBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>prezioaSartuBean</managed-bean-name>
		<managed-bean-class>eredua.bean.PrezioaSartuBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>MainGUI.xhtml</display-name>
		<from-view-id>/MainGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>1</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MainGUI.xhtml</display-name>
		<from-view-id>/MainGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>2</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MainGUI.xhtml</display-name>
		<from-view-id>/MainGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>3</from-outcome>
			<to-view-id>/GuestMainGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Register.xhtml</display-name>
		<from-view-id>/Register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Driver</from-outcome>
			<to-view-id>/DriverMainGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>DriverMainGUI.xhtml</display-name>
		<from-view-id>/DriverMainGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/CreateRideGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRideGUI.xhtml</display-name>
		<from-view-id>/CreateRideGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>close</from-outcome>
			<to-view-id>/MainGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRideGUI.xhtml</display-name>
		<from-view-id>/CreateRideGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/CreateRideGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRideGUI.xhtml</display-name>
		<from-view-id>/CreateRideGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>close</from-outcome>
			<to-view-id>/DriverMainGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRideGUI.xhtml</display-name>
		<from-view-id>/CreateRideGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/CreateRideGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>DriverMainGUI.xhtml</display-name>
		<from-view-id>/DriverMainGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/QueryRidesGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>GuestMainGUI.xhtml</display-name>
		<from-view-id>/GuestMainGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/QueryRidesGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Traveler</from-outcome>
			<to-view-id>/TravelerMainGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>TravelerMainGUI.xhtml</display-name>
		<from-view-id>/TravelerMainGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/QueryRidesGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MainGUI.xhtml</display-name>
		<from-view-id>/MainGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>4</from-outcome>
			<to-view-id>/PrezioaSartuGUI.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>PrezioaSartuGUI.xhtml</display-name>
		<from-view-id>/PrezioaSartuGUI.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/PreziokoBidaiakErakutsi.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
